{
  "name": "d3-force-surface",
  "version": "0.5.6",
  "description": "A multi-surface elastic collision force type for the d3-force simulation engine.",
  "keywords": [
    "d3",
    "d3-module",
    "d3-force",
    "surface",
    "bounce",
    "impact",
    "collision",
    "physics",
    "layout",
    "graph",
    "force",
    "simulation"
  ],
  "homepage": "https://github.com/vasturiano/d3-force-surface",
  "license": "MIT",
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "unpkg": "dist/d3-force-surface.min.js",
  "main": "dist/d3-force-surface.common.js",
  "module": "dist/d3-force-surface.module.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/d3-force-surface.git"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/d3-force-surface.js -o dist/d3-force-surface.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "terser": "^4.6.6"
  }
}
